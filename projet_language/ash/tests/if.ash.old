a = 5
if a == 5 then
    writeln("a == 5 (if)")
else
    writeln("never")
end

a = 6
if a == 5 then
    writeln("never")
else
    writeln("a == 6 (else after if)")
end

a = 7
if a == 5 then
    writeln("never")
elif a == 6 then
    writeln("never")
else
    writeln("a == 7 (else after elif)")
end

a = 8
if a == 5 then
    writeln("never")
elif a == 6 then
    writeln("never")
elif a == 8 then
    writeln("a == 8 (elif)")
end
